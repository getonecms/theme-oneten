{
    "name": "getonecms/theme-oneten",
    "description": "OneTen is an OneCMS's official front-end default theme.",
    "type": "onecms-theme",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Shifreen",
            "email": "mshifreen@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0",
        "getonecms/ext-base": "*",
        "getonecms/ext-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "OneTen\\": "src/"
        }
    },
    "optimize-autoloader": true,
    "repositories": [
        {
            "type": "path",
            "url": "../ext-base"
        },
        {
            "type": "path",
            "url": "../ext-admin"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}